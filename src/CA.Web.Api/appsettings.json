{
  "ConnectionStrings": {
    "PersistenceConnectionForMysqlDb": "Server=localhost;Port=3306;Database=CA;Uid=root;Pwd=abc-1234;"
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "restrictedToMinimumLevel": "Information"
      },
      {
        "Name": "File",
        "Args": {
          "path": "serilogs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Application": "Clean Architecture"
    }
  },
  "AllowedHosts": "*"
}
